{
  "pages": [
    "pages/tabbar/element/element",
    "pages/tabbar/assembly/assembly",
    "pages/tabbar/template/template",
    "pages/tabbar/about/about"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#5CA3E1",
    "navigationBarTitleText": "CC UI",
    "navigationBarTextStyle": "white"
  },
  "subpackages": [
    {
      "root": "pages/element/",
      "pages": [
        "layout/layout",
        "text/text",
        "color/color",
        "shadow/shadow",
        "icon/icon",
        "identity/identity",
        "button/button",
        "tag/tag",
        "loading/loading",
        "progress/progress"
      ]
    },
    {
      "root": "pages/assembly/",
      "pages": [
        "navbar/navbar",
        "tabbar/tabbar",
        "notice/notice",
        "timeline/timeline",
        "steps/steps",
        "swiper/swiper",
        "numberbox/numberbox",
        "rolling/rolling",
        "rate/rate",
        "options/options",
        "countdown/countdown",
        "toast/toast",
        "timepricker/timepricker",
        "suspension/suspension",
        "collapse/collapse"
      ]
    },
    {
      "root": "pages/template/",
      "pages": [
        "index/index"
      ]
    },
    {
      "root": "pages/about/",
      "pages": [
        "index/index"
      ]
    }
  ],
  "tabBar": {
    "color": "#bfbfbf",
    "selectedColor": "#5CA3E1",
    "backgroundColor": "#FFFFFF",
    "list": [
      {
        "pagePath": "pages/tabbar/element/element",
        "text": "元素",
        "iconPath": "/images/tab/element.png",
        "selectedIconPath": "/images/tab/element-active.png"
      },
      {
        "pagePath": "pages/tabbar/assembly/assembly",
        "text": "组件",
        "iconPath": "/images/tab/assembly.png",
        "selectedIconPath": "/images/tab/assembly-active.png"
      },
      {
        "pagePath": "pages/tabbar/template/template",
        "text": "模板",
        "iconPath": "/images/tab/template.png",
        "selectedIconPath": "/images/tab/template-active.png"
      },
      {
        "pagePath": "pages/tabbar/about/about",
        "text": "关于",
        "iconPath": "/images/tab/about.png",
        "selectedIconPath": "/images/tab/about-active.png"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  },
  "usingComponents": {
    "Custom": "/components/custom/custom"
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于定位效果展示"
    }
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json"
}