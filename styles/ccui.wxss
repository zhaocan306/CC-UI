/*********
  圆角边框
*********/
.round {
	border-radius: 5000rpx;
}
.radius {
	border-radius: 6rpx;
}
/*********
  操作条
*********/
.cc-bar {
  display: flex;
	position: relative;
	align-items: center;
	min-height: 100rpx;
	justify-content: space-between;
  border-bottom: 1rpx solid #eaeef1;
}
.cc-bar .action {
  display: flex;
	align-items: center;
	height: 100%;
	justify-content: center;
	max-width: 100%;
  font-size: 30rpx;
}
.cc-bar .action:first-child>text[class*="icon-"] {
	margin-left: -0.3em;
	margin-right: 0.3em;
}
.cc-bar .action>text[class*="icon-"],
.cc-bar .action>view[class*="icon-"] {
	font-size: 36rpx;
}
/*********
  瀑布流布局
*********/
.cc-waterfall {
  padding: 0 24rpx;
  display: flex;
}
.cc-waterfall .cc-waterfall-list {
  flex: 1;
  margin-right: 48rpx;
  padding-bottom: 48rpx;
}
.cc-waterfall .cc-waterfall-list:nth-child(2n) {
  margin-right: 0rpx;
}
.cc-waterfall .cc-waterfall-item {
  flex: 1;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  margin-top: 48rpx;
  color: var(--white);
  position: relative;
}
.cc-waterfall-list [class*="icon-"] {
	font-size: 52rpx;
  height: 60rpx;
  line-height: 60rpx;
}
/*********
  标签
*********/
.cc-tag {
  font-size: 24rpx;
	vertical-align: middle;
	position: relative;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	box-sizing: border-box;
	padding: 0rpx 16rpx;
	height: 48rpx;
	font-family: Helvetica Neue, Helvetica, sans-serif;
	white-space: nowrap;
}
.cc-tag.badge {
	border-radius: 200rpx;
	position: absolute;
	top: -10rpx;
	right: -10rpx;
	font-size: 20rpx;
	padding: 0rpx 10rpx;
	height: 28rpx;
	color: var(--white);
}
.cc-tag[class*="text-"] {
  min-width: 32rpx;
	height: 32rpx;
	top: -4rpx;
	right: -4rpx;
  font-size: 20rpx;
}
.cc-tag[class*="icon-"] {
  width: 32rpx;
	height: 32rpx;
	top: -4rpx;
	right: -4rpx;
}
/*********
  头像
*********/
.cc-btn {
  position: relative;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	box-sizing: border-box;
	padding: 0 30rpx;
	font-size: 28rpx;
	height: 64rpx;
  line-height: 64rpx;
	text-align: center;
	text-decoration: none;
	overflow: visible;
	margin-left: initial;
	transform: translate(0rpx, 0rpx);
	margin-right: initial;
}
.cc-btn::after {
  border: 0;
  display: none;
}
.cc-btn.sm {
	padding: 0 20rpx;
	font-size: 20rpx;
	height: 48rpx;
  line-height: 48rpx;
}
.cc-btn.lg {
	padding: 0 40rpx;
	font-size: 32rpx;
	height: 80rpx;
  line-height: 80rpx;
}
.cc-btn[class*="line-"] {
	background-color: transparent;
}
.cc-btn[class*="line-"]::after {
	content: " ";
	display: block;
	width: 200%;
	height: 200%;
	position: absolute;
	top: 0;
	left: 0;
	border: 1rpx solid currentColor;
	transform: scale(0.5);
	transform-origin: 0 0;
	box-sizing: border-box;
	border-radius: 12rpx;
	z-index: 1;
	pointer-events: none;
}
.cc-btn.round[class*="line-"]::after {
	border-radius: 1000rpx;
}
.cc-btn.block {
  width: 100%;
	display: flex;
}
.cc-btn[disabled] {
	opacity: 0.6;
	color: var(--white)!important;
}
