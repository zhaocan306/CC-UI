.cc-load {
	display: block;
	line-height: 3em;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
}
.cc-load::before {
	font-family: "element-icons";
	display: inline-block;
	margin-right: 6rpx;
}
.cc-load.loading::before {
	content: "\e8bd";
	animation: cuIcon-spin 2s infinite linear;
}
.cc-load.loading::after {
	content: "加载中...";
}
.cc-load.over::before {
	content: "\e624";
}
.cc-load.over::after {
	content: "没有更多了";
}
.cc-load.erro::before {
	content: "\e70d";
}
.cc-load.erro::after {
	content: "加载失败";
}
@keyframes cuIcon-spin {
	0% {
		-webkit-transform: rotate(0);
		transform: rotate(0);
	}

	100% {
		-webkit-transform: rotate(359deg);
		transform: rotate(359deg);
	}
}
