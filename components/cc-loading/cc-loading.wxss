  url('//at.alicdn.com/t/font_864725_2qkq20t2fim.ttf?t=1584067599656') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_864725_2qkq20t2fim.svg?t=1584067599656#element-icons') format('svg'); /* iOS 4.1- */
